<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
    <route id="TeamRaceWebIndex" pattern="/">
    	<default key="_controller">TeamRaceWebBundle:Main:index</default>
    </route>
    <route id="TeamRaceWebAbout" pattern="/about">
    	<default key="_controller">TeamRaceWebBundle:Main:about</default>
    </route>
    <route id="TeamRaceWebLogin" pattern="/login">
    	<default key="_controller">TeamRaceWebBundle:Main:login</default>
    </route>
    <route id="TeamRaceWebRegister" pattern="/register">
    	<default key="_controller">TeamRaceWebBundle:Main:register</default>
    </route>
    <route id="TeamRaceWebLoginCheck" pattern="/login_check">
    </route>
</routes>
