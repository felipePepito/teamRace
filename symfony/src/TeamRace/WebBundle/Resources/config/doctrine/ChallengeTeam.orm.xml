<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\ChallengeTeam" table="challenge_team">
    <indexes>
      <index name="fk_challenge_team_1_idx" columns="id_challenge"/>
      <index name="fk_challenge_team_2_idx" columns="id_team"/>
    </indexes>
    <id name="idChallengeTeam" type="integer" column="id_challenge_team">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="points" type="float" column="points" precision="10" scale="0" nullable="true"/>
    <many-to-one field="idTeam" target-entity="Teams">
      <join-columns>
        <join-column name="id_team" referenced-column-name="id_team"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idChallenge" target-entity="Challenges">
      <join-columns>
        <join-column name="id_challenge" referenced-column-name="id_challenge"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
