<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\UserTeam" table="user_team">
    <indexes>
      <index name="fk_user_team_1_idx" columns="id_user"/>
      <index name="fk_user_team_2_idx" columns="id_team"/>
    </indexes>
    <id name="idUserTeam" type="integer" column="id_user_team">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="integer" column="role" nullable="false"/>
    <many-to-one field="idTeam" target-entity="Team">
      <join-columns>
        <join-column name="id_team" referenced-column-name="id_team"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUser" target-entity="User">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
