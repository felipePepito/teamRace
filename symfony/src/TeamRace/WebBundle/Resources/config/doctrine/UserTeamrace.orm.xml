<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\UserTeamrace" table="user_teamrace">
    <indexes>
      <index name="fk_user_teamrace_1_idx" columns="id_user"/>
      <index name="fk_user_teamrace_2_idx" columns="id_teamrace"/>
    </indexes>
    <id name="idUserTeamrace" type="integer" column="id_user_teamrace">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="integer" column="role" nullable="false"/>
    <many-to-one field="idTeamrace" target-entity="Teamraces">
      <join-columns>
        <join-column name="id_teamrace" referenced-column-name="id_teamrace"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUser" target-entity="User">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
