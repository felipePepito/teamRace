<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\Teamraces" table="teamraces">
    <indexes>
      <index name="fk_teamrace_1_idx" columns="id_creator"/>
    </indexes>
    <id name="idTeamrace" type="integer" column="id_teamrace">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="datecreated" type="datetime" column="dateCreated" nullable="false"/>
    <field name="image" type="string" column="image" length="255" nullable="true"/>
    <many-to-one field="idCreator" target-entity="User">
      <join-columns>
        <join-column name="id_creator" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
