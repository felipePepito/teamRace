<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\Blog" table="blog">
    <indexes>
      <index name="fk_blogs_1_idx" columns="id_teamrace"/>
    </indexes>
    <id name="idBlog" type="integer" column="id_blog">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="headline" type="string" column="headline" length="255" nullable="false"/>
    <field name="text" type="text" column="text" nullable="false"/>
    <many-to-one field="idTeamrace" target-entity="Teamrace">
      <join-columns>
        <join-column name="id_teamrace" referenced-column-name="id_teamrace"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
