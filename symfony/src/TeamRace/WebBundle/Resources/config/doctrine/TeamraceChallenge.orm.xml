<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\TeamraceChallenge" table="teamrace_challenge">
    <indexes>
      <index name="fk_teamrace_challenge_1_idx" columns="id_teamrace"/>
      <index name="fk_teamrace_challenge_2_idx" columns="id_challenge"/>
      <index name="fk_teamrace_challenge_3_idx" columns="id_tutor"/>
    </indexes>
    <id name="idTeamraceChallenge" type="integer" column="id_teamrace_challenge">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="maxPoints" type="float" column="max_points" precision="10" scale="0" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <many-to-one field="idTutor" target-entity="User">
      <join-columns>
        <join-column name="id_tutor" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idChallenge" target-entity="Challenges">
      <join-columns>
        <join-column name="id_challenge" referenced-column-name="id_challenge"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTeamrace" target-entity="Teamraces">
      <join-columns>
        <join-column name="id_teamrace" referenced-column-name="id_teamrace"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
