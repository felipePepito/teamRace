<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TeamRace\WebBundle\Entity\Team" table="team">
    <indexes>
      <index name="fk_teams_1_idx" columns="id_teamrace"/>
    </indexes>
    <id name="idTeam" type="integer" column="id_team">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="image" type="string" column="image" length="255" nullable="true"/>
    <field name="slogan" type="string" column="slogan" length="2048" nullable="true"/>
    <many-to-one field="idTeamrace" target-entity="Teamrace">
      <join-columns>
        <join-column name="id_teamrace" referenced-column-name="id_teamrace"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
